<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="wheelchair">
  <xacro:arg name="gpu" default="false"/>
  <xacro:property name="gpu" value="$(arg gpu)" />
  <!-- Base footprint joint -->
   <link name="base_footprint"/>
    <joint name="base_link_joint" type="fixed">
     <parent link="base_footprint"/>
     <child link="base_link"/>
     <origin xyz="0 0 0.100"/>
   </joint>
  <!-- Base Link -->
  <link name="base_link">
   <!-- Base Link visual -->
    <visual>
      <origin rpy="0 0 3.1415" xyz="0.22 0 0.01"/>
      <geometry>
        <mesh filename="package://electric_wheelchair/mesh/base.dae"/>
      </geometry>
    </visual>
    <!-- Base Link collision -->
    <collision>
      <origin rpy="0 0 3.1415" xyz="0.22 0 0.01"/>
      <!-- <origin rpy="0 0 0" xyz="0.226 0.00 0.150"/> -->
      <geometry>
        <mesh filename="package://electric_wheelchair/mesh/base.dae"/>
        <!-- <box size="0.592 0.492 0.300"/> -->
      </geometry>
    </collision>
  <!-- Base Link inertial -->
   <inertial>
    <origin xyz="0 0 0"/>
    <mass value="48.000"/>
    <inertia
      ixx="0.051" ixy="0" ixz="0"
      iyy="0.107" iyz="0"
      izz="0.106"/>
    </inertial>
  </link>

  <!-- Left wheel -->
  <!-- Left wheel link -->
  <link name="left_wheel_link">
    <!-- Left wheel link visual -->
    <visual>
      <geometry>
        <mesh filename="package://electric_wheelchair/mesh/rearwheel.dae"/>
        <!-- <cylinder radius="0.1325" length="0.055"/> -->
      </geometry>
      <origin xyz="-0.22 0 -0.25" rpy="1.5709 0 0"/>
      <material name="black"/>
    </visual>
    <!-- Left wheel link collision -->
    <collision>
      <geometry>
        <mesh filename="package://electric_wheelchair/mesh/rearwheel.dae"/>
        <!-- <cylinder radius="0.1325" length="0.05"/> -->
      </geometry>
      <origin xyz="-0.22 0 -0.25" rpy="1.5709 0 0"/>
    </collision>

    <!-- Left wheel link inertial -->
   <inertial>
    <origin xyz="0 0 0"/>
    <mass value="1.000"/>
    <!-- <inertia ixx="0.0013541667" ixy="0" ixz="0" iyy="0.0013541667" iyz="0" izz="0.0025"/> -->
    <inertia
        ixx="0.004433" ixy="0" ixz="0"
        iyy="0.004433" iyz="0"
        izz="0.00845"/>
   </inertial>

  </link>
  <!-- Left wheel joint -->
  <joint name="left_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="left_wheel_link"/>
    <origin rpy="-1.570796 0 0" xyz="0 0.246 0"/>
    <axis xyz="0 0 1"/>
  </joint>

  <!-- Right wheel -->
  <!-- Right wheel link -->
  <link name="right_wheel_link">
    <!-- Right wheel link visual -->
    <visual>
      <geometry>
        <mesh filename="package://electric_wheelchair/mesh/rearwheel.dae"/>
        <!-- <cylinder radius="0.1325" length="0.055"/> -->
      </geometry>
      <origin xyz="-0.23 0 -0.25" rpy="1.5709 0 0"/>
      <material name="black"/>
    </visual>
  <!-- Right wheel link collision -->
    <collision>
      <geometry>
        <mesh filename="package://electric_wheelchair/mesh/rearwheel.dae"/>
        <!-- <cylinder radius="0.1325" length="0.055"/> -->
      </geometry>
      <origin xyz="-0.23 0 -0.25" rpy="1.5709 0 0"/>
    </collision>
    <!-- Right wheel link inertial -->
   <inertial>
    <origin xyz="0 0 0"/>
    <mass value="1.000"/>
    <!-- <inertia ixx="0.0013541667" ixy="0" ixz="0" iyy="0.0013541667" iyz="0" izz="0.0025"/> -->
    <inertia
        ixx="0.004433" ixy="0" ixz="0"
        iyy="0.004433" iyz="0"
        izz="0.00845"/>
   </inertial>

  </link>
<!-- Right wheel joint -->
  <joint name="right_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="right_wheel_link"/>
    <origin rpy="1.5707 0 0" xyz="0 -0.246 0"/>
    <axis xyz="0 0 -1"/>
  </joint>

  <!-- Left caster -->
  <!-- Left caster link -->
  <link name="left_caster_link">
    <!-- Left caster link visual -->
    <visual>
      <geometry>
        <mesh filename="package://electric_wheelchair/mesh/frontwheel.dae"/>
        <!-- <cylinder radius="0.127" length="0.055"/> -->
      </geometry>
      <origin xyz="0.36 0 0.24" rpy="1.5707 0 0"/>
      <!-- <origin xyz="0 0 0" rpy="1.5707 0 0"/> -->
      <material name="green">
        <color rgba="0 1 0 1"/>
      </material>
    </visual>
    <!-- Left caster link collision -->
    <collision>
      <geometry>
        <mesh filename="package://electric_wheelchair/mesh/frontwheel.dae"/>
        <!-- <cylinder radius="0.127" length="0.055"/> -->
      </geometry>
      <origin xyz="0.36 0 0.24" rpy="1.5707 0 0"/>
    </collision>
<!-- Left caster link inertial -->
   <inertial>
    <origin xyz="0 0 0"/>
    <mass value="1.000"/>
    <inertia
        ixx="0.004433"  ixy="0" ixz="0"
        iyy="0.004433"  iyz="0"
        izz="0.0007812"/>
   </inertial>

  </link>
 <!-- Left caster joint -->
  <joint name="left_caster_joint" type="continuous">
   <parent link="base_link"/>
   <child link="left_caster_link"/>
   <origin rpy="1.5707 0 0" xyz="0.592 0.246 0"/>
   <axis xyz="0 0 1"/>
  </joint>


  <!-- Right caster -->
  <!-- Right caster link -->
 <link name="right_caster_link">
  <!-- Right caster link visual -->
   <visual>
     <geometry>
       <mesh filename="package://electric_wheelchair/mesh/frontwheel.dae"/>
       <!-- <cylinder radius="0.127" length="0.055"/> -->
     </geometry>
     <origin xyz="0.36 0 0.23" rpy="1.5707 0 0"/>
          <!-- <origin xyz="0 0 0" rpy="1.5707 0 0"/> -->
     <material name="green">
       <color rgba="0 1 0 1"/>
     </material>
   </visual>
  <!-- Right caster link collision -->
   <collision>
     <geometry>
      <mesh filename="package://electric_wheelchair/mesh/frontwheel.dae"/>
       <!-- <cylinder radius="0.127" length="0.055"/> -->
     </geometry>
     <origin xyz="0.36 0 0.23" rpy="1.5707 0 0"/>
   </collision>
  <!-- Right caster link inertial -->
  <inertial>
   <origin xyz="0 0 0"/>
   <mass value="1.000"/>
    <inertia
        ixx="0.004433"  ixy="0" ixz="0"
        iyy="0.004433"  iyz="0"
        izz="0.0007812"/>
  </inertial>

 </link>
<!-- Right caster joint -->
 <joint name="right_caster_joint" type="continuous">
  <parent link="base_link"/>
  <child link="right_caster_link"/>
  <origin rpy="1.5707 0 0" xyz="0.592 -0.246 0"/>
  <axis xyz="0 0 1"/>
  </joint>


<!-- ===============  Transmission =============== -->
  <transmission name="left_wheel_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="left_wheel_joint">
      <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    </joint>
    <actuator name="left_wheel_motor">
      <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
      <mechanicalReduction>30</mechanicalReduction>
    </actuator>
  </transmission>

 <transmission name="right_wheel_trans">
  <type>transmission_interface/SimpleTransmission</type>
  <joint name="right_wheel_joint">
    <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
  </joint>
  <actuator name="right_wheel_motor">
    <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    <mechanicalReduction>30</mechanicalReduction>
  </actuator>
 </transmission>

<!-- =============== Gazebo =============== -->
  <gazebo>
   <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
    <robotNamespace>wheelchair</robotNamespace>
   </plugin>
  </gazebo>

 <gazebo reference="base_link">
  <selfCollide>true</selfCollide>
  <mu1 value="0.05" />
  <mu2 value="0.05" />
 </gazebo>

 <gazebo reference="left_wheel_link">
  <selfCollide>true</selfCollide>
  <mu1 value="0.8" />
  <mu2 value="0.8" />
 </gazebo>

 <gazebo reference="right_wheel_link">
  <selfCollide>true</selfCollide>
  <mu1 value="0.8" />
  <mu2 value="0.8" />
 </gazebo>

 <gazebo reference="left_caster_link">
  <selfCollide>true</selfCollide>
  <!-- This caster is no frictional resistance. -->
  <mu1 value="0.8" />
  <mu2 value="0.8" />
 </gazebo>

 <gazebo reference="right_caster_link">
  <selfCollide>true</selfCollide>
  <!-- This caster is no frictional resistance. -->
  <mu1 value="0.8" />
  <mu2 value="0.8" />
 </gazebo>


  <xacro:include filename="$(find velodyne_description)/urdf/VLP-16.urdf.xacro"/>
  <VLP-16
      parent="base_link"
      name="velodyne"
      topic="/velodyne_points"
      hz="10"
      samples="440"
      gpu="${gpu}">
      <origin xyz="0 0 1" rpy="0 0 0" />
  </VLP-16>

</robot>
